{
  "name": "vite-template-doly",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:no-mock": "cross-env MOCK=none vite",
    "build": "tsc && vite build",
    "build:analyze": "cross-env GENERATE_SOURCEMAP=true BUILD_PATH=dist-analyze vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:style": "stylelint src/**/*.{css,less}",
    "preview": "vite preview",
    "analyze": "npm run build:analyze && source-map-explorer 'dist-analyze/assets/*.js'",
    "prettier": "prettier -w **/*",
    "prepare": "husky",
    "commit": "cz",
    "test": "vitest"
  },
  "lint-staged": {
    "**/*.{css,less}": "stylelint --fix strict",
    "**/*.{js,mjs,jsx,ts,tsx}": "eslint",
    "**/*.{css,scss,less,js,mjs,jsx,ts,tsx,json,md}": "prettier -w"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "axios": "^1.12.2",
    "cache2": "^3.1.2",
    "classnames": "^2.5.1",
    "rc-hooks": "^3.1.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.9.1",
    "react-transition-group": "^4.4.5",
    "ut2": "^1.21.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/cz-commitlint": "^19.8.1",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.18.6",
    "@types/react": "^18.3.24",
    "@types/react-dom": "^18.3.7",
    "@types/react-transition-group": "^4.4.12",
    "@vitejs/plugin-react-swc": "^3.11.0",
    "autoprefixer": "^10.4.21",
    "commitizen": "^4.3.1",
    "cross-env": "^7.0.3",
    "cssnano": "^7.1.1",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.21",
    "globals": "^16.4.0",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "less": "^4.4.1",
    "lint-staged": "^15.5.2",
    "mockjs-extend": "^1.7.0",
    "postcss": "^8.5.6",
    "postcss-less": "^6.0.0",
    "prettier": "^3.6.2",
    "prettier-plugin-two-style-order": "^1.0.1",
    "source-map-explorer": "^2.5.3",
    "stylelint": "^16.24.0",
    "stylelint-config-css-modules": "^4.5.1",
    "stylelint-config-standard": "^38.0.0",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "stylelint-no-unsupported-browser-features": "^8.0.4",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.44.1",
    "vite": "^6.3.6",
    "vite-plugin-mock-dev-server": "^1.9.3",
    "vitest": "^3.2.4",
    "web-vitals": "^4.2.4"
  },
  "engines": {
    "node": ">=18"
  }
}
