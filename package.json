{
  "name": "vite-template-doly",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:no-mock": "cross-env MOCK=none vite",
    "build": "tsc && vite build",
    "build:analyze": "cross-env GENERATE_SOURCEMAP=true BUILD_PATH=dist-analyze vite build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "lint:style": "stylelint src/**/*.{css,less}",
    "preview": "vite preview",
    "analyze": "npm run build:analyze && source-map-explorer 'dist-analyze/assets/*.js'",
    "prettier": "prettier -w **/*",
    "prepare": "husky",
    "commit": "cz",
    "test": "vitest"
  },
  "lint-staged": {
    "**/*.{css,less}": "stylelint --fix",
    "**/*.{js,mjs,jsx,ts,tsx}": "eslint",
    "**/*.{css,scss,less,js,mjs,jsx,ts,tsx,json,md}": "prettier -w"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "axios": "^1.7.9",
    "cache2": "^3.1.2",
    "classnames": "^2.5.1",
    "rc-hooks": "^3.0.16",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router": "^7.1.1",
    "react-transition-group": "^4.4.5",
    "ut2": "^1.15.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.6.1",
    "@commitlint/config-conventional": "^19.6.0",
    "@commitlint/cz-commitlint": "^19.6.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^22.10.5",
    "@types/react": "^18.3.18",
    "@types/react-dom": "^18.3.5",
    "@types/react-transition-group": "^4.4.12",
    "@vitejs/plugin-react-swc": "^3.7.2",
    "autoprefixer": "^10.4.20",
    "commitizen": "^4.3.1",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.6",
    "eslint": "^9.17.0",
    "eslint-plugin-react-hooks": "^5.1.0",
    "eslint-plugin-react-refresh": "^0.4.16",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "inquirer": "^9.3.7",
    "jsdom": "^25.0.1",
    "less": "^4.2.1",
    "lint-staged": "^15.3.0",
    "mockjs-extend": "^1.3.2",
    "postcss": "^8.4.49",
    "postcss-less": "^6.0.0",
    "prettier": "^3.4.2",
    "prettier-plugin-two-style-order": "^1.0.1",
    "source-map-explorer": "^2.5.3",
    "stylelint": "^16.12.0",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "stylelint-no-unsupported-browser-features": "^8.0.2",
    "typescript": "^5.7.2",
    "typescript-eslint": "^8.19.0",
    "vite": "^5.4.11",
    "vite-plugin-mock-dev-server": "^1.8.3",
    "vitest": "^2.1.8",
    "web-vitals": "^4.2.4"
  },
  "engines": {
    "node": ">=20"
  }
}
