{
  "name": "vite-template-doly",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "dev:no-mock": "cross-env MOCK=none vite",
    "build": "tsc && vite build",
    "build:analyze": "cross-env GENERATE_SOURCEMAP=true BUILD_PATH=dist-analyze vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "lint:style": "stylelint src/**/*.{css,less}",
    "preview": "vite preview",
    "analyze": "npm run build:analyze && source-map-explorer 'dist-analyze/assets/*.js'",
    "prettier": "prettier -w **/*",
    "prepare": "husky",
    "commit": "cz",
    "test": "vitest"
  },
  "lint-staged": {
    "**/*.{css,less}": "stylelint --fix",
    "**/*.{js,mjs,jsx,ts,tsx}": "eslint",
    "**/*.{css,scss,less,js,mjs,jsx,ts,tsx,json,md}": "prettier -w"
  },
  "config": {
    "commitizen": {
      "path": "@commitlint/cz-commitlint"
    }
  },
  "dependencies": {
    "axios": "^1.7.2",
    "cache2": "^3.0.0",
    "classnames": "^2.5.1",
    "rc-hooks": "^3.0.11",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.24.0",
    "react-transition-group": "^4.4.5",
    "ut2": "^1.9.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@commitlint/cz-commitlint": "^19.2.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^15.0.7",
    "@testing-library/user-event": "^14.5.2",
    "@types/node": "^20.14.9",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/react-transition-group": "^4.4.10",
    "@typescript-eslint/eslint-plugin": "^7.14.1",
    "@typescript-eslint/parser": "^7.14.1",
    "@vitejs/plugin-react-swc": "^3.7.0",
    "autoprefixer": "^10.4.19",
    "commitizen": "^4.3.0",
    "cross-env": "^7.0.3",
    "cssnano": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-plugin-react-hooks": "^4.6.2",
    "eslint-plugin-react-refresh": "^0.4.7",
    "husky": "^9.0.11",
    "inquirer": "^9.3.0",
    "jsdom": "^24.1.0",
    "less": "^4.2.0",
    "lint-staged": "^15.2.7",
    "mockjs-extend": "^1.3.1",
    "postcss": "^8.4.38",
    "postcss-less": "^6.0.0",
    "prettier": "^3.3.2",
    "prettier-plugin-two-style-order": "^1.0.1",
    "source-map-explorer": "^2.5.3",
    "stylelint": "^16.6.1",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-declaration-block-no-ignored-properties": "^2.8.0",
    "stylelint-no-unsupported-browser-features": "^8.0.1",
    "typescript": "^5.5.2",
    "vite": "^5.3.1",
    "vite-plugin-mock-dev-server": "^1.5.0",
    "vitest": "^1.6.0",
    "web-vitals": "^4.2.0"
  }
}
